<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/secondaryProgress">
    <shape
        android:innerRadiusRatio="3"
        android:shape="ring"
        android:thicknessRatio="21.0"
        android:useLevel="true">
            <solid android:color="#00111111"/>
        </shape>
    </item>

    <item android:id="@android:id/progress">
        <rotate
            android:fromDegrees="270"
            android:pivotX="50%"
            android:pivotY="50%"
            android:toDegrees="1200">
            <shape
                android:innerRadiusRatio="3"
                android:shape="ring"
                android:thicknessRatio="21.0"
                android:useLevel="true">

                <rotate
                    android:fromDegrees="0"
                    android:pivotX="50%"
                    android:pivotY="50%"
                    android:toDegrees="700"/>
                <gradient
                    android:centerColor="@color/ieci_primary_green"
                    android:endColor="@color/ieci_primary_blue"
                    android:startColor="@color/ieci_secondary_green"
                    android:type="sweep" />
            </shape>
        </rotate>
    </item>

</layer-list>